{
  "name": "solana-superapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "check:types": "tsc --noEmit -p tsconfig.all.json",
    "check:frontend": "tsc --noEmit -p tsconfig.json",
    "check:errors": "node scripts/analyze-typescript-errors.js",
    "check:errors:quick": "node scripts/quick-type-check.js",
    "check:errors:detailed": "node scripts/analyze-typescript-errors.js --detailed",
    "check:errors:critical": "node scripts/analyze-typescript-errors.js --priority-only",
    "check:errors:all": "node scripts/analyze-typescript-errors.js --config=tsconfig.all.json --detailed",
    "check:errors:save": "node scripts/analyze-typescript-errors.js --output=typescript-errors-report.md --format=md",
    "fix:types": "node scripts/auto-fix-types.js",
    "fix:types:dry": "node scripts/auto-fix-types.js --dry-run",
    "fix:types:safe": "node scripts/auto-fix-types.js --no-backup",
    "setup:type-checking": "node scripts/setup-type-checking.js",
    "tunnel": "node -e \"console.log(' Доступные туннели:\\n  npm run tunnel:ngrok\\n  npm run tunnel:cloudflare\\n  npm run tunnel:lt')\"",
    "tunnel:ngrok": "ngrok http 3000",
    "tunnel:cloudflare": "cloudflared tunnel --url http://localhost:3000",
    "tunnel:lt": "npx localtunnel --port 3000 --subdomain solana-superapp-dev",
    "setup:tunnels": "npm install -g @ngrok/ngrok cloudflared localtunnel",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:jobs": "tsx prisma/seed-jobs.ts",
    "db:reset": "npx prisma migrate reset",
    "db:generate": "npx prisma generate"
  },
  "dependencies": {
