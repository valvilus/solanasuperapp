    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@storacha/client": "^1.7.9",
    "@twa-dev/sdk": "^8.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/three": "^0.179.0",
