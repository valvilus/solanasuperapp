    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "prisma": "^6.14.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
